{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: g++.exe build active file",
			"command": "C:\\msys64\\mingw64\\bin\\g++.exe",
			"args": [
				"-std=c++20",
				"-g",
				"${file}",
				"src\\core\\net.cpp",
				"src\\image_processing\\image_processing.cpp",
				"src\\interface\\application_interface.cpp",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe",
				"-I","C:\\opencv\\opencv_4.5.0\\include",
				"-I","${workspaceFolder}",
                "-L","C:\\opencv\\opencv_4.5.0\\x64\\mingw\\bin",                
                "-llibopencv_calib3d450",
                "-llibopencv_core450",
                "-llibopencv_dnn450",
                "-llibopencv_features2d450",
				"-llibopencv_flann450",
                "-llibopencv_highgui450",
                "-llibopencv_imgcodecs450",
                "-llibopencv_imgproc450",
                "-llibopencv_ml450",
                "-llibopencv_objdetect450",
                "-llibopencv_photo450",
                "-llibopencv_stitching450",
                "-llibopencv_video450",
				"-llibopencv_videoio450"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}